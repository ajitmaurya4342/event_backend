{
  "name": "reporting-server",
  "version": "1.0.0",
  "description": "reporting-server",
  "scripts": {
    "start": "npm run build:tsc && cross-env NODE_ENV=production node dist/src/index.js",
    "start:tsc": "npm run build:tsc && cross-env NODE_ENV=production node dist/src/index.js",
    "start:swc": "npm run build:swc && cross-env NODE_ENV=production node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:swc": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .js,.cjs,.mjs,.ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build:tsc && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.brij.tech/cinematic/reporting-server.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@auth0/s3": "^1.0.0",
    "@godaddy/terminus": "^4.12.1",
    "aws-sdk": "^2.1450.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.8.1",
    "json-stringify-deterministic": "^1.0.10",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.5.1",
    "knex-paginate": "^3.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.5.2",
    "objection": "^3.0.4",
    "path-to-regexp": "^6.2.1",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-http-print": "^3.1.0",
    "pino-pretty": "^10.2.0",
    "type-fest": "^4.0.0",
    "uuid": "^3.3.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
